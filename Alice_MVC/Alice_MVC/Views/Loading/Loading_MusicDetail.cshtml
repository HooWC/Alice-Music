@{
	ViewData["Title"] = "Loading_MusicDetail";
	Layout = "Loading_Layout";
}

<h1 style="display:none;" id="videoid">@ViewBag.Id</h1>

<div id="load">
	<div>G</div>
	<div>N</div>
	<div>I</div>
	<div>D</div>
	<div>A</div>
	<div>O</div>
	<div>L</div>
</div>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
<script>

	var s = null;

	$(function () {


		s = $("#videoid").html();

		window.setInterval("countDown()", 2000);

	})

	function countDown() {
		window.location.href = `/Music/MusicDetail?SongID=${s}`;
	}

</script>

